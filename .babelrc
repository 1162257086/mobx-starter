{
  "plugins": [
    "add-module-exports",
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-es2015-modules-commonjs",
    "transform-es2015-spread",
    "transform-react-jsx",
    ["fast-async", {
      "env": {"dontMapStackTraces": true},
      "runtimePattern": "index\\.js"
    }]
  ]
}
